<script>
    $(function () {
        daoh();
    })

    function daoh() {
        var $nav = $("#navbar li");
        $nav.click(function () {
            $(this).find(".ej_show_box").toggle();
        });
    };

    $(function () {
        var $nav = $("#navbar li");
        $nav.hover(function () {
            $(this).find(".ej_show_box").css({ "display": "block" });
        }, function () {
            $(this).find(".ej_show_box").css({ "display": "none" });
        });

    });

</script>
<script>
    $(function () {
        var t = $(".recommend-date li p").text().length;
        $(".recommend-date li p").each(function () {
            var t = $(this).text();
            var tt = $(this).text().length;
            if (tt > 5) {
                t = t.substring(0, 5);
                $(this).html(t + "...");

            } else {
                $(this).html(t);
            }
        })

    })
</script>
