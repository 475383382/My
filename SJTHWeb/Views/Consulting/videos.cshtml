                        foreach (var item in Model)
                        {
                                <li class="current" style="margin-top: 0px;">
                                    <a href="@item.url" class="pic" target="PC">
                                        
                                        <embed src="@item.url" width="100" height="70" />
                                        <p style="line-height:1.5; height:50px; font-size:12px; color:#dedede">@item.name</p>
                                    </a>
                                    </li>
                            
                        }
                    }
                    </ul>
                </div>
                <div class="recommend-operation"><span class="prev"><i class="icon">上一页</i></span><span class="next"><i class="icon">下一页</i></span></div>
            </div>
        </div>
        <!-- //recommend-box -->
